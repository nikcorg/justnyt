RewriteEngine on
RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . index.php [L]

SetEnv APP_ENVIRONMENT production

# Uncomment line below to enter maintenance mode
# SetEnv MAINTENANCE 1

# Comment the line below to disable dev settings
# SetEnv APP_ENVIRONMENT dev

AddDefaultCharset UTF-8
